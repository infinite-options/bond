<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BondMobileApp.App">
    <Application.Resources>


        <ResourceDictionary>
            <Style x:Key="FontLabelStyle" TargetType="Label">
                <Setter Property="FontFamily" Value="Chrome"/>
            </Style>
            <Style x:Key="FontLabelRighteous" TargetType="Label">
                <Setter Property="FontFamily" Value="Righteous"/>
            </Style>
            <Style x:Key="FontLabelStaat" TargetType="Label">
                <Setter Property="FontFamily" Value="Staat"/>
            </Style>
        </ResourceDictionary>


    </Application.Resources>
</Application>